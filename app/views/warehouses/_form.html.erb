  <%= form_with model: warehouse, local: true do |f| %>

  <div class="row">
    <div class="col-lg-6">
      <div class="form-group">
      <%= f.label :street %>
      <%= f.text_field :street, class: 'form-control' %>
      <%= render_error(warehouse, :street) %>
  </div>
    </div>
    <div class="col-lg">
      <div class="form-group">
      <%= f.label :city %>
      <%= f.text_field :city, class: 'form-control' %>
      <%= render_error(warehouse, :city) %>
  </div>  
    </div>
    <div class="col-lg">
      <div class="form-group">
      <%= f.label :province %>
      <%= f.text_field :province, class: 'form-control' %>
      <%= render_error(warehouse, :province) %>
  </div>
    </div>
  </div>



  

  <%= f.submit 'Save Warehouse', class: 'btn btn-success', id: 'warehouse-edit-submit' %>
  <%= link_to 'Back', warehouses_path, class: 'btn btn-primary', id: 'warehouse-edit-back' %>
<% end %>