  <%= form_with model: warehouse, remote: false do |f| %>

  <div class="row">
    <div class="col-lg-6">
      <div class="form-group">
      <%= f.label :street %>
      <%= f.text_field :street, class: 'form-control' %>
  </div>
    </div>
    <div class="col-lg">
      <div class="form-group">
      <%= f.label :city %>
      <%= f.text_field :city, class: 'form-control' %>
  </div>  
    </div>
    <div class="col-lg">
      <div class="form-group">
      <%= f.label :province %>
      <%= f.text_field :province, class: 'form-control' %>
  </div>
    </div>
  </div>



  

  <%= f.submit 'Save Warehouse', class: 'btn btn-success' %>
  <%= link_to 'Back', warehouses_path, class: 'btn btn-primary' %>
<% end %>