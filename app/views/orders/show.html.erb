<h1>WELCOME TO SHOW</h1>

<div class="container">
  <dl class="row">
    <dt class="col-sm-2">Name</dt>
    <dd class="col-sm-10"><%= @order.customer_name %></dd>

    <dt class="col-sm-2">Warehouse</dt>
    <dd class="col-sm-10"><%= "#{@order.warehouse.province}, #{@order.warehouse.city}, #{@order.warehouse.street}" %></dd>
  </dl>
  <div class="row">
    <%= link_to 'Back', orders_path, class: 'btn btn-primary' %>
  </div>
</div>
<p><%= @order %></p>